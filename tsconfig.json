{
  "compilerOptions": {
    "outDir": "./dist/",
    "jsx": "react",
    "target": "es6",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "baseUrl": "./src/",
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.js", "**/*.ts", "**/*.tsx"]
}